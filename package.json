{
  "name": "json-property-filter",
  "version": "1.2.0",
  "keywords": [
    "json-property-filter",
    "filter",
    "json",
    "property",
    "path",
    "object",
    "element",
    "include",
    "exclude"
  ],
  "description": "A library to filter a JSON object by including/excluding properties.",
  "main": "./lib/jsonPropertyFilter.js",
  "scripts": {
    "build": "tsc",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "tslint src/**/*.ts",
    "prepublish": "npm run build",
    "test": "mocha --compilers ts:ts-node/register --recursive test",
    "test:coverage": "nyc --require ts-node/register _mocha --compilers ts:ts-node/register --recursive test/unit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyrilschumacher/json-property-filter.git"
  },
  "bugs": {
    "url": "https://github.com/cyrilschumacher/json-property-filter/issues"
  },
  "engines": {
    "node": ">=5.0"
  },
  "homepage": "https://github.com/cyrilschumacher/json-property-filter",
  "typings": "./lib/jsonPropertyFilter.d.ts",
  "author": "Cyril Schumacher",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/commander": "^2.9.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.7",
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "mocha": "^3.4.2",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "commander": "^2.10.0"
  }
}
